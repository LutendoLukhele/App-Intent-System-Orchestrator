{
  "name": "aso",
  "version": "1.0.0",
  "private": true,
  "description": "ASO - App-System-Orchestrator: Intent Orchestration Infrastructure",
  "author": "Lutendo Lukhele",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com//aso.git"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:packages": "turbo run build --filter='./packages/*'",
    "build:legacy": "tsc -p . && cpx \"src/prompts/**/*\" \"dist/prompts\"",
    "dev": "turbo run dev --parallel",
    "dev:legacy": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "turbo run test",
    "test:legacy": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:cortex": "jest tests/cortex --verbose",
    "test:cortex:infra": "jest tests/cortex/1-infrastructure.test.ts --verbose",
    "test:cortex:cache": "jest tests/cortex/2-cache.test.ts --verbose --testTimeout=30000",
    "test:cortex:routing": "jest tests/cortex/3-routing.test.ts --verbose --testTimeout=30000",
    "test:cortex:webhooks": "jest tests/cortex/4-webhooks.test.ts --verbose --testTimeout=30000",
    "test:cortex:e2e": "jest tests/cortex/5-e2e.test.ts --verbose --testTimeout=60000",
    "test:cortex:performance": "jest tests/cortex/6-performance.test.ts --verbose --testTimeout=120000",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules",
    "interpret": "ts-node src/scripts/interpret.ts --",
    "debug:nango": "npx ts-node scripts/debug-nango-connection.ts"
  },
  "keywords": [
    "aso",
    "orchestrator",
    "intent",
    "automation",
    "reactive",
    "llm",
    "ai"
  ],
  "dependencies": {
    "@lixpi/markdown-stream-parser": "^0.0.3-32",
    "@nangohq/node": "^0.58.5",
    "@neondatabase/serverless": "^1.0.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.69.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.211.0",
    "@opentelemetry/resources": "^2.5.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "@opentelemetry/sdk-trace-node": "^2.5.0",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "@redis/client": "^5.0.1",
    "@types/joi": "^17.2.2",
    "ajv": "^8.17.1",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "cpx": "^1.5.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.5.0",
    "env": "^0.0.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "firebase": "^11.7.1",
    "firebase-admin": "^13.4.0",
    "groq-sdk": "^0.19.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "joi": "^18.0.2",
    "lodash": "^4.17.21",
    "mammoth": "^1.6.0",
    "marked": "^9.1.6",
    "multer": "^1.4.5-lts.1",
    "node-persist": "^4.0.4",
    "openai": "^4.97.0",
    "pdf-parse": "^1.1.1",
    "prom-client": "^15.1.3",
    "react-router-dom": "^7.13.0",
    "stripe": "^14.0.0",
    "uuid": "^11.1.0",
    "winston": "^3.18.3",
    "ws": "^8.18.1",
    "xlsx": "^0.18.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.12",
    "@types/marked": "^5.0.2",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.14.1",
    "@types/node-persist": "^3.1.8",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19.2.10",
    "@types/ws": "^8.18.1",
    "@types/xlsx": "^0.0.35",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "turbo": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@10.0.0"
}
